#include <stdio.h>
#include <string.h>2
 
using namespace std;
 
char a[20]; 
int n;  
 
void Gray(int k)
{
    if (k == n)
    {
        printf("%s\n", a);
        return ;
    }
 
    Gray(k+1);
    a[k] = '0' + '1' - a[k];    
    Gray(k+1);
}
 
int main()
{
   printf("N = "); 
   scanf("%d",&n);
   for(int i=0;i<n;i++)
   {
   	a[i]='0';
   }
   	 Gray(0);  
    printf("%s\n",a);
    return 0;
}
